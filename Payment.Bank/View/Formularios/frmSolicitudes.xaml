                                    <ComboBox Grid.Row="5" Grid.Column="0" Height="25" Width="94" HorizontalAlignment="Right" x:Name="cmbExpirationYear" SelectedValue="{Binding Path=Ano, Mode=TwoWay, ValidatesOnDataErrors=True, ValidatesOnExceptions=True}" Margin="2,0,2,0"/>
                                </StackPanel>

                                <TextBlock Text="lblCvv" Grid.Row="4" Grid.Column="1"/>
                            </Grid>
                        </StackPanel>

                    </Grid>

