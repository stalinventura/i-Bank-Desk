<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Payment.Bank"
        xmlns:Viewer="clr-namespace:SAPBusinessObjects.WPF.Viewer;assembly=SAPBusinessObjects.WPF.Viewer"
        x:Class="Payment.Bank.View.Informes.frmPrintEntradasGetByEntradaID"
        mc:Ignorable="d"
        Title="frmLogIn" Height="768" Width="1024" WindowStartupLocation="CenterScreen" WindowStyle="None" AllowsTransparency="True" BorderThickness="1"  BorderBrush="{StaticResource BackgroundHeader}">
    <Grid x:Name="root" >
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="{StaticResource BackgroundHeader}" Stroke="Black" Margin="-2"/>
        <Button HorizontalAlignment="Right" x:Name="btnSalir" Margin="0,10,10,0" VerticalAlignment="Top" Width="30" Height="30">
            <Image  HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Height="30" Source="/Images/cancel.png" />
        </Button>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Informe" Foreground="White" FontSize="18" VerticalAlignment="Top" Width="122"/>
        <Viewer:CrystalReportsViewer Name="ReportViewer" Grid.Row="1" ToggleSidePanel="None" ShowToggleSidePanelButton="False" ShowLogo="False" ShowRefreshButton="False" ShowCopyButton="False"/>
    </Grid>
</Window>
